{
  "name": "giraffe-syntax-highlighter",
  "displayName": "Giraffe Syntax",
  "description": "Code highlights for script based language Giraffe",
  "version": "0.0.2",
  "icon": "./images/icon-giraffe.png",
  "publisher": "JellyTyan",
  "repository": {
    "type": "git",
    "url": "https://github.com/NullPointerGang/vscode-giraffe"
  },
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "giraffe",
      "aliases": ["Giraffe", "giraffe"],
      "extensions": [".gfe"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "giraffe",
      "scopeName": "source.giraffe",
      "path": "./syntaxes/giraffe.tmLanguage.json"
    }]
  }
}