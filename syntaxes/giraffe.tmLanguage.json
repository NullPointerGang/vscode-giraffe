{
	"patterns": [
	  {
		"name": "comment.block.documentation.giraffe",
		"begin": "/\\*",
		"end": "\\*/",
		"patterns": [{ "name": "comment.block.documentation.giraffe", "match": ".*?" }]
	  },
	  {
		"name": "comment.line.double-slash.giraffe",
		"match": "//.*$"
	  },
	  {
		"name": "storage.type.giraffe",
		"match": "\\b(int|float|str|Null|list|dict|Option)\\b"
	  },
	  {
		"name": "variable.language.giraffe",
		"match": "\\b(a|b|ops|users|user)\\b"
	  },
	  {
		"name": "constant.numeric.giraffe",
		"match": "\\b\\d+\\.\\d+|\\d+\\b"
	  },
	  {
		"name": "string.quoted.double.giraffe",
		"begin": "\"",
		"end": "\"",
		"patterns": [{ "name": "string.quoted.double.giraffe", "match": ".*?" }]
	  },
	  {
		"name": "meta.function.giraffe",
		"match": "\\b(func|return|void)\\b"
	  },
	  {
		"name": "keyword.operator.giraffe",
		"match": "\\b(->)\\b"
	  },
	  {
		"name": "meta.type.giraffe",
		"match": "\\bOption\\[[a-zA-Z,\\s]*\\]$"
	  },
	  {
		"name": "operator.giraffe",
		"match": "\\b(=|\\+|\\-|\\*|\\/|==|<=|>=|=<|=\\>)\\b"
	  },
	  {
		"name": "punctuation.separator.giraffe",
		"match": "[(){}\\[\\],;]"
	  }
	]
  }